  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"src/**/*.ts\" --fix",
    "migration:format": "prettier --write \"migrations/*.ts\"",
    "migration:create": "typeorm migration:create && yarn migration:format",
    "migration:generate": "npx ts-node node_modules/.bin/typeorm migration:generate -d ormconfig.ts && yarn migration:format",
    "migration:run": "npx ts-node node_modules/.bin/typeorm migration:run -d ormconfig.ts && yarn migration:format"
  },
